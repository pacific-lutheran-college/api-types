{
  "name": "@plc/tassapi-types-2026-03",
  "version": "2026.3.0",
  "description": "Generated TypeScript types for TASS API 2026.03",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "generate": "openapi-typescript ./spec/openapi-spec.json -o ./src/generated/tassapi.ts",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepare": "npm run generate && npm run build"
  },
  "keywords": [
    "typescript",
    "types",
    "tass",
    "openapi"
  ],
  "license": "ISC",
  "devDependencies": {
    "openapi-typescript": "^7.6.1",
    "typescript": "^5.8.3"
  }
}
