{
  "name": "tassapi-types-2026-03",
  "version": "1.0.0",
  "description": "TypeScript types for TASS API v2026.03",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./projections": {
      "import": "./dist/projections/index.js",
      "require": "./dist/projections/index.js",
      "types": "./dist/projections/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "npm run build",
    "generate": "openapi-typescript ../../specs/tassapi-2026-03.json -o src/generated.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "openapi-typescript": "^7.13.0",
    "typescript": "^5.9.3"
  }
}
