{
  "name": "tassapi-zod-2026-03",
  "version": "1.0.0",
  "description": "Zod schemas for TASS API v2026.03",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "npm run build",
    "generate": "openapi-zod-client ../../specs/tassapi-2026-03.json -o src/generated.ts -t ../../node_modules/openapi-zod-client/src/templates/schemas-only.hbs",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "openapi-zod-client": "^1.18.3",
    "typescript": "^5.9.3"
  }
}
